<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1230.0"
	xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Pane layoutX="14.0" layoutY="14.0" prefHeight="93.0"
			prefWidth="1205.0" style="-fx-background-color: #aeffff;">
			<children>
				<Label layoutX="499.0" layoutY="29.0" text="Member Register"
					textFill="#6b6565">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Label>
			</children>
		</Pane>
		<TextField fx:id="txt_name" layoutX="532.0" layoutY="191.0"
			prefHeight="35.0" prefWidth="244.0" promptText="Name">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</TextField>
		<Button layoutX="615.0" layoutY="522.0" mnemonicParsing="false"
			onAction="#btn_register_click" prefHeight="26.0" prefWidth="78.0"
			style="-fx-background-color: #00ff00;" text="Register"
			textFill="#323030">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<Label layoutX="533.0" layoutY="591.0" text="Existing Member?" />
		<Label layoutX="650.0" layoutY="589.0"
			onMouseClicked="#lbl_login_card_click" text="Login Card Here!"
			textFill="#2402fc">
			<font>
				<Font name="System Bold Italic" size="16.0" />
			</font>
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Label>
		<Button layoutX="1098.0" layoutY="669.0"
			mnemonicParsing="false" onAction="#btn_logout_click"
			prefHeight="26.0" prefWidth="102.0"
			style="-fx-background-color: #00676c;" text="Log Out"
			textFill="WHITE">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<Button layoutX="45.0" layoutY="669.0" mnemonicParsing="false"
			onAction="#btn_back_click" prefHeight="26.0" prefWidth="102.0"
			style="-fx-background-color: #55557f;" text="Back" textFill="WHITE">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<Label layoutX="381.0" layoutY="198.0" text="Name">
			<font>
				<Font size="16.0" />
			</font>
		</Label>
		<TextField fx:id="txt_roll_no" layoutX="532.0"
			layoutY="275.0" prefHeight="35.0" prefWidth="244.0"
			promptText="Roll No.">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</TextField>
		<Label layoutX="386.0" layoutY="282.0" text="Roll No.">
			<font>
				<Font size="16.0" />
			</font>
		</Label>
		<Label layoutX="381.0" layoutY="444.0" text="Year">
			<font>
				<Font size="16.0" />
			</font>
		</Label>
		<ComboBox fx:id="cbo_year" layoutX="535.0" layoutY="437.0"
			prefHeight="35.0" prefWidth="244.0" promptText="Choose Year">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</ComboBox>
		<TextField fx:id="txt_academic_year" layoutX="532.0"
			layoutY="360.0" prefHeight="35.0" prefWidth="244.0"
			promptText="Academic Year (eg: 2016 - 2017)">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</TextField>
		<Label layoutX="381.0" layoutY="367.0" text="Academic Year">
			<font>
				<Font size="16.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
