<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1230.0"
	xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Pane layoutX="14.0" layoutY="14.0" prefHeight="93.0"
			prefWidth="1205.0" style="-fx-background-color: #aeffff;">
			<children>
				<Label layoutX="516.0" layoutY="29.0" text="Member Login"
					textFill="#6b6565">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Label>
			</children>
		</Pane>
		<TextField fx:id="txt_card_id" layoutX="475.0"
			layoutY="314.0" prefHeight="35.0" prefWidth="269.0"
			promptText="Card Id">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</TextField>
		<Button layoutX="570.0" layoutY="386.0" mnemonicParsing="false"
			onAction="#btn_login_click" prefHeight="26.0" prefWidth="78.0"
			style="-fx-background-color: #0cebff;" text="Login"
			textFill="#323030">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<Button layoutX="666.0" layoutY="386.0" mnemonicParsing="false"
			onAction="#btn_clear_click" prefHeight="26.0" prefWidth="78.0"
			style="-fx-background-color: #ff0000;" text="Clear"
			textFill="#323030">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<Label layoutX="510.0" layoutY="454.0" text="Not card yet?" />
		<Label layoutX="611.0" layoutY="452.0"
			onMouseClicked="#lbl_register_card_click" text="Register Card Here!"
			textFill="#2402fc">
			<font>
				<Font name="System Bold Italic" size="16.0" />
			</font>
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Label>
		<Label layoutX="521.0" layoutY="246.0"
			text="Input Member Card ID">
			<font>
				<Font name="System Bold" size="16.0" />
			</font>
		</Label>
		<Button layoutX="1098.0" layoutY="669.0"
			mnemonicParsing="false" onAction="#btn_logout_click"
			prefHeight="26.0" prefWidth="102.0"
			style="-fx-background-color: #00676c;" text="Log Out"
			textFill="WHITE">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<Button layoutX="45.0" layoutY="669.0" mnemonicParsing="false"
			onAction="#btn_back_click" prefHeight="26.0" prefWidth="102.0"
			style="-fx-background-color: #55557f;" text="Back" textFill="WHITE">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
	</children>
</AnchorPane>
