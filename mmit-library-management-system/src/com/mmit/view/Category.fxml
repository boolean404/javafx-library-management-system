<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1230.0"
	xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.mmit.controller.CategoryController">
	<children>
		<Pane layoutX="89.0" layoutY="136.0" prefHeight="462.0"
			prefWidth="384.0" style="-fx-background-color: #dbffed;">
			<children>
				<TextField fx:id="txt_category" layoutX="66.0"
					layoutY="151.0" prefHeight="42.0" prefWidth="253.0"
					promptText="Category">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</TextField>
				<Button layoutX="74.0" layoutY="238.0"
					mnemonicParsing="false" onAction="#btn_add_click" prefHeight="26.0"
					prefWidth="102.0" style="-fx-background-color: #1ccaff;" text="Add"
					textFill="WHITE">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
				<Button layoutX="199.0" layoutY="238.0"
					mnemonicParsing="false" onAction="#btn_delete_click"
					prefHeight="26.0" prefWidth="102.0"
					style="-fx-background-color: #ff5500;" text="Delete"
					textFill="WHITE">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
				<Label layoutX="66.0" layoutY="116.0" text="Category Name"
					textFill="#6b6666">
					<font>
						<Font size="16.0" />
					</font>
				</Label>
			</children>
		</Pane>
		<Button layoutX="1091.0" layoutY="668.0"
			mnemonicParsing="false" onAction="#btn_logout_click"
			prefHeight="26.0" prefWidth="102.0"
			style="-fx-background-color: #00676c;" text="Log Out"
			textFill="WHITE">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<Button layoutX="38.0" layoutY="668.0" mnemonicParsing="false"
			onAction="#btn_back_click" prefHeight="26.0" prefWidth="102.0"
			style="-fx-background-color: #55557f;" text="Back" textFill="WHITE">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<Pane layoutX="15.0" layoutY="25.0" prefHeight="83.0"
			prefWidth="1200.0" style="-fx-background-color: #aeffff;">
			<children>
				<Label layoutX="536.0" layoutY="24.0" text="Categories"
					textFill="#514d4d">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Label>
			</children>
		</Pane>
		<ScrollPane layoutX="521.0" layoutY="136.0"
			prefHeight="462.0" prefWidth="651.0">
			<content>
				<AnchorPane minHeight="0.0" minWidth="0.0"
					prefHeight="200.0" prefWidth="200.0">
					<children>
						<TableView fx:id="tbl_category" prefHeight="459.0"
							prefWidth="651.0">
							<columns>
								<TableColumn fx:id="col_id" prefWidth="113.0"
									text="Id" />
								<TableColumn fx:id="col_name" prefWidth="185.0"
									text="Name" />
								<TableColumn fx:id="col_created_at"
									prefWidth="185.0" text="Created At" />
								<TableColumn fx:id="col_created_by" minWidth="0.0"
									prefWidth="167.0" text="Created By" />
							</columns>
						</TableView>
					</children>
				</AnchorPane>
			</content>
		</ScrollPane>
	</children>
</AnchorPane>
